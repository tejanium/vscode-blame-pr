{
	"publisher": "tejanium",
	"name": "blame-pr",
	"displayName": "Blame Github's Pull Request",
	"icon": "img/ico.png",
	"description": "Blame and open Github's associated PR.",
	"version": "1.0.1",
	"homepage": "https://github.com/tejanium/vscode-blame-pr/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/tejanium/vscode-blame-pr/issues"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/tejanium/vscode-blame-pr"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:blame-pr.open",
		"onCommand:blame-pr.toggleStatusbar"
	],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "blame-pr.open",
				"title": "Blame PR: Open in Github"
			},
			{
				"command": "blame-pr.toggleStatusbar",
				"title": "Blame PR: Toggle Status Bar"
			}
		],
		"configuration": {
			"title": "Blame Github's PR",
			"properties": {
				"blame-pr.githubToken": {
					"type": "string",
					"default": "",
					"description": "Github personal access token with access to repo."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"lint": "eslint src --ext ts",
		"watch": "webpack --mode none --watch",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"pretest": "yarn compile && yarn test-compile && yarn lint",
		"test": "node ./out/test/runTest.js",
		"test-compile": "tsc -p ./",
		"deploy": "vsce publish --yarn"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/sinon": "^9.0.1",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"mock-spawn": "^0.2.6",
		"nock": "^12.0.3",
		"sinon": "^9.0.2",
		"ts-loader": "^7.0.4",
		"typescript": "^3.8.3",
		"vsce": "^1.75.0",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@octokit/graphql": "^4.4.0",
		"await-spawn": "^3.0.0",
		"vscode-cache": "^0.3.0"
	}
}
